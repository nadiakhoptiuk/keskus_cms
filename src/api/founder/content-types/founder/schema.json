{
  "kind": "collectionType",
  "collectionName": "founders",
  "info": {
    "singularName": "founder",
    "pluralName": "founders",
    "displayName": "Founder",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "maxLength": 40,
      "minLength": 3
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true,
      "minLength": 3
    },
    "email": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "email",
      "required": true,
      "minLength": 3
    },
    "phone_displaying": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "minLength": 3,
      "maxLength": 14,
      "regex": "^(([0-9\\+])+\\s)+([0-9])+$",
      "required": true
    },
    "phone_link": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "minLength": 3,
      "maxLength": 14,
      "required": true,
      "regex": "^([0-9\\+])+$"
    },
    "photo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "image.image",
      "required": true
    }
  }
}
